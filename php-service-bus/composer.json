{
  "name": "php-service-bus/performance-comparison",
  "description": "PHP Service Bus skeleton",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "php-service-bus/service-bus": "v4.0.*",
    "php-service-bus/module-phpinnacle-transport": "v4.0.*",
    "php-service-bus/module-storage-sql": "v4.0.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "app/AppExtension.php",
      "tools/DemoPublisher.php"
    ]
  }
}
